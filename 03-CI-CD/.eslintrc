/* prettier-ignore */
{
    "extends": ["eslint:recommended", "plugin:react/recommended", "plugin:jest/recommended", "prettier"],
    "plugins": ["react", "jest"],
    "env": {
      "browser": true,
      "es2021": true,
      "jest/globals": true
    },
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "settings": {
      "react": {
        "version": "detect"  // Diese Zeile f√ºgt die automatische Erkennung der React-Version hinzu
      }
    },
    "rules": {
      "prettier/prettier": ["error"]
    }
  }